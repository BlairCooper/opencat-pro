<div class="fade modal" role=dialog aria-labelledby=alertModalLabel id=alertModal tabindex=-1>
    <div class=modal-dialog role=document>
        <div class=modal-content>
            <div class=modal-header>
                <h4 class=modal-title id=alertModalLabel><%= titleText %></h4></div>
            <div class=modal-body><%= bodyText %></div>
            <div class=modal-footer>
                <button class="btn btn-default" data-dismiss=modal type=button>Ok</button>
            </div>
        </div>
    </div>
</div>
